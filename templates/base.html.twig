<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Welcome!{% endblock %}</title>
        {% block stylesheets %}
            {{ encore_entry_link_tags('base') }}
        {% endblock %}
        {% block stylesheetsCustom %}{% endblock %}

        <script>
            (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
                (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
                m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
            })(window,document,'script','https://www.google-analytics.com/analytics.js','ga');

            ga('create', '{{ ga_tracking }}', 'auto');
            ga('send', 'pageview');
        </script>

    </head>
    <body>

        <div class="container-fluid row">
            <div class="col-2">
                {% block colLeft %}{% endblock %}
            </div>
            <div class="col-8">
                {% include 'navbar.html.twig' %}
                {% block body %}{% endblock %}
            </div>
            <div class="col-2">
                {% block colRight %}{% endblock %}
            </div>
        </div>

        {% block javascripts %}
            <script src="https://www.google.com/recaptcha/api.js?render={{ google_recaptcha_site }}"></script>
            {{ encore_entry_script_tags('base') }}
        {% endblock %}
        {% block javascriptsCustom %}
        {% endblock %}

    </body>
</html>
